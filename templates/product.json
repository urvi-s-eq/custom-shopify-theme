        "quote_mp37a4": {
          "type": "quote",
          "settings": {
            "quote_image": "shopify://shop_images/review-slider-img3.jpg",
            "quote_text": "<p>“Bombing Science has great prices and variety. Dang brand paint is great quality and sold for a great price. I would strongly recommend Bombing Science for art/graff supplies!”</p>",
            "author_name": "Matthew Keyes"
          }
        },
        "quote_4Uq6aB": {
