        "quote_MV6Tar": {
          "type": "quote",
          "settings": {
            "quote_image": "shopify://shop_images/review-slider-img1.jpg",
            "quote_text": "<p>“Great customer service. I had damage to a order, they fixed/ resolved immediately! Also the search tool on shop website is very user friendly (dont need exact text to generate results)!”</p>",
