        "quote_rwMBCX": {
          "type": "quote",
          "settings": {
            "quote_image": "shopify://shop_images/review-slider-img2.jpg",
            "quote_text": "<p>“I just found Bombing Science and I’m glad I said! Great website! Quick ship! Easy to navigate. Great selection and better prices than the big guy! I’m a Bombing Science customer! Thanks!”</p>",
            "author_name": "stevie kay"
